{
  "name": "svn-simple-auth-provider",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/svn-simple-auth-provider.mjs",
  "module": "src/svn-simple-auth-provider.mjs",
  "description": "provide credentials from subversion simple auth",
  "keywords": [
    "auth-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/svn-simple-auth-provider.mjs --section=API",
    "lint": "documentation lint src/svn-simple-auth-provider.mjs",
    "posttest": "markdown-doctest && npm run prepare",
    "test": "ava"
  },
  "dependencies": {
    "kv-reader": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "c8": "^4.1.4",
    "documentation": "^10.1.0",
    "esm": "^3.2.22",
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^15.13.12"
  },
  "engines": {
    "node": ">=10.15.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svn-simple-auth-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svn-simple-auth-provider/issues"
  },
  "homepage": "https://github.com/arlac77/svn-simple-auth-provider#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "js",
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
